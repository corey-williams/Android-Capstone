<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/createnew"
    tools:ignore="HardcodedText" >

    <TextView
        android:id="@+id/textQuest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="Name:"
        android:typeface="normal" />

    <TextView
        android:id="@+id/shopGoldValue1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textQuest"
        android:text="Race:" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/shopGoldValue1"
        android:text="Class:" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView3"
        android:text="Title:" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView4"
        android:text="Alliance:" />


    <TextView
        android:id="@+id/textViewStrName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textQuest"
        android:text=" " />


    <TextView
        android:id="@+id/textViewStrRace"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView3"
        android:layout_toRightOf="@+id/textView3"
        android:text=" " />


    <TextView
        android:id="@+id/TextViewStrClass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView3"
        android:layout_alignBottom="@+id/textView3"
        android:layout_toRightOf="@+id/textQuest"
        android:text=" " />

    <TextView
        android:id="@+id/textViewStrTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView5"
        android:layout_toRightOf="@+id/textView4"
        android:text=" Commoner" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textView11"
        android:layout_below="@+id/textView11"
        android:text="HP:" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textView12"
        android:layout_below="@+id/textView12"
        android:text="MP:" />

    <TextView
        android:id="@+id/textViewLevelValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView12"
        android:text="1" />

    <TextView
        android:id="@+id/textViewHP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView12"
        android:layout_alignBottom="@+id/textView12"
        android:layout_toRightOf="@+id/textView12"
        android:text="10" />

    <TextView
        android:id="@+id/textViewMP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView13"
        android:layout_alignBottom="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="10" />


    <TextView
        android:id="@+id/textViewStrAlliance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toRightOf="@+id/textView5"
        android:text=" " />

    <Spinner
        android:id="@+id/spinnerMonsters"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/checkBox1"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="54dp"
        android:entries="@array/Monsters"
        android:prompt="@string/MonsterPrompt" />


    <Button
        android:id="@+id/btnBattle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/spinnerMonsters"
        android:layout_alignParentRight="true"
        android:text="Battle" />

    <CheckBox
        android:id="@+id/checkBox1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/btnBattle"
        android:layout_toLeftOf="@+id/btnBattle"
        android:text="Auto" />

    <TextView
        android:id="@+id/textViewBattleResults"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:layout_below="@+id/textView10"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/btnBattle"/>

    <Button
        android:id="@+id/btnStats"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_toLeftOf="@+id/textView6"
        android:text="Stats" />

    <Button
        android:id="@+id/btnAchievements"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@+id/textView6"
        android:text="Achievements" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btnOption3"
        android:layout_centerHorizontal="true"
        android:text="|"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:visibility="invisible" />

    <Button
        android:id="@+id/btnInventory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btnStats"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/btnStats"
        android:text="Inventory" />

    <Button
        android:id="@+id/btnCrafting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btnAchievements"
        android:layout_alignLeft="@+id/btnAchievements"
        android:layout_alignParentRight="true"
        android:text="Class Training" />

    <Button
        android:id="@+id/btnEquipment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btnInventory"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/btnInventory"
        android:text="Equipment" />

    <Button
        android:id="@+id/btnShop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btnCrafting"
        android:layout_alignLeft="@+id/btnCrafting"
        android:layout_alignParentRight="true"
        android:text="Shop" />

    <Button
        android:id="@+id/btnStoryMode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btnEquipment"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/btnEquipment"
        android:text="Story Mode" />

    <Button
        android:id="@+id/btnQuests"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btnShop"
        android:layout_alignLeft="@+id/btnShop"
        android:layout_alignParentRight="true"
        android:text="Quests" />

    <TextView
        android:id="@+id/textViewStartCheck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btnStoryMode"
        android:layout_alignParentLeft="true"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textViewShopPurchased1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textViewStartCheck"
        android:layout_alignBottom="@+id/textViewStartCheck"
        android:layout_alignParentLeft="true"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textViewTimer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btnBattle"
        android:layout_alignBottom="@+id/btnBattle"
        android:layout_toRightOf="@+id/textView13"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textViewLevelTimer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textViewStartCheck"
        android:layout_alignParentLeft="true"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textViewTierTimer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textViewStartCheck"
        android:layout_alignBottom="@+id/textViewStartCheck"
        android:layout_alignParentLeft="true"
        android:text="TextView"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textViewTierCheck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textViewStartCheck"
        android:layout_alignBottom="@+id/textViewStartCheck"
        android:layout_toRightOf="@+id/textViewStartCheck"
        android:text="T0"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textViewGoldValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0" android:layout_alignTop="@+id/textView8" android:layout_toRightOf="@+id/textView8"/>

    <TextView
        android:id="@+id/textViewXPValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0" android:layout_alignTop="@+id/textView" android:layout_toRightOf="@+id/textView6"/>

    <TextView
        android:id="@+id/textViewXpNextLevel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="100"
        android:layout_alignTop="@+id/textView7" android:layout_toRightOf="@+id/textView7"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/"
        android:layout_below="@+id/textViewStrAlliance" android:layout_toRightOf="@+id/textViewXPValue"
        android:layout_marginTop="11dp"/>


    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="XP:"
            android:id="@+id/textView"
            android:layout_alignTop="@+id/textView7" android:layout_toLeftOf="@+id/textView6"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Gold:"
            android:id="@+id/textView8"
            android:layout_alignTop="@+id/textViewStrTitle" android:layout_toLeftOf="@+id/textViewMP"/>

    <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Continue fighting"
            android:id="@+id/ContinueBattle"
            android:layout_below="@+id/spinnerMonsters"
            android:layout_centerHorizontal="true"
            android:layout_alignBottom="@+id/textView10"
            android:minWidth="100dp"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Small Text"
            android:id="@+id/textView9"
            android:layout_below="@+id/spinnerMonsters"
            android:layout_toRightOf="@+id/ContinueBattle"
            android:visibility="invisible"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Small Text"
            android:id="@+id/textView10"
            android:visibility="invisible"
            android:layout_below="@+id/textView9"
            android:layout_toRightOf="@+id/ContinueBattle"/>

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="TextViewww"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView14"
        android:text="Level:" />

</RelativeLayout>